{
  "name": "fingerate-world",
  "version": "1.0.0",
  "description": "VivifyIdeas Nest Boilerplate",
  "author": "Nikola Spalevic",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" && eslint --fix --ext .ts .",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "clear_jest": "jest --clearCache",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --verbose --detectOpenHandles --forceExit",
    "test:cov": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --coverage --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "webpack": "webpack --config webpack.config.js",
    "ts-typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:create": "typeorm migration:create -d src/migrations",
    "migration:run": "yarn ts-typeorm migration:run",
    "migration:revert": "yarn ts-typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^8.0.3",
    "authenticator": "^1.1.5",
    "aws-sdk": "^2.1204.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bull": "^4.9.0",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "coingecko-api-v3": "^0.0.13",
    "country-state-city": "^3.0.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.0.0",
    "firebase-admin": "^11.2.0",
    "geojson": "^0.5.0",
    "js-big-decimal": "^1.3.5",
    "messagebird": "^3.8.1",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^3.1.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.2.45",
    "typeorm-extension": "^1.1.0",
    "typescript": "^4.6.2",
    "uuid": "^9.0.0",
    "web3": "^1.7.5",
    "web3-eth-contract": "^1.7.5",
    "web3-utils": "^1.7.5",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.4.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.0.0",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^27.4.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/speakeasy": "^2.0.7",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.0",
    "supertest": "^6.2.4",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
